<#setting classic_compatible=true>
<@insert template="layout/layer">
    <div class="list_page">
    	<input id="rowId" type="hidden" value="${rowId}"></input>
        <table>
            <thead>
                <tr>
                    <td width="16%">批次号</td>
                    <td width="16%">生产日期</td>
                    <td width="16%">过期日期</td>
                </tr>
            </thead>
        </table>
        <div class="data_grid" style="height:280px;">
            <table>
                <tbody id="show_batch_number">
                </tbody>
            </table>
        </div>
    </div>
</@insert>