@charset "utf-8";
/** tab样式 **/
div.tab-control{position:relative;margin-left:1px;margin-right:3px;height:100%;}
div.tab-control *{font:normal normal 12px/24px "微软雅黑";}
div.tab-control ul,div.tab-control li{position:relative; padding-left:0; }
div.tab-control ul{overflow:hidden;margin:0;}
div.tab-control li{cursor:pointer;list-style-type:none;margin:2px 5px 0 0;}
div.tab-control a{text-indent:-999999px;}
div.tab-control input{outline:none;border:none;background:none;}
div.tab-control div.tab{position:relative;}
div.tab-control div.tab input{position:absolute;width:24px;height:30px;background-image:url("../icons/tab-control_tab_input.png");}
div.tab-control div.tab input.prev{left:0;background-position:0 0;}
div.tab-control div.tab input.next{right:30px;background-position:-40px 0;}
div.tab-control div.tab input.find{right:0;background-position:-80px 0;}
div.tab-control div.tab ul{height:35px;}
div.tab-control div.tab ul li{float:left;width:140px;height:30px;background-image:url("../icons/tab-control_tab_li.png");line-height:32px;text-indent:10px;border-top-left-radius: 5px;border-top-right-radius: 5px;border: 3px solid #60a7ff; border-bottom:0px; color:#323232; background-position:0 -50px;}
div.tab-control div.tab ul a{position:absolute;right:8px;top:8px;width:15px;height:15px;background-image:url("../icons/tab-control_tab_li.png");background-position:-140px 25px;}div.tab-control div.tab-find{position:absolute;z-index:2;right:0;top:0;padding:4px;box-shadow:0 0 2px #000;background-color:#f3f3f3;}
div.tab-control div.tab-find form{width:174px;height:24px;background-image:url("../icons/tab-control_tab-find_form.png");}
div.tab-control div.tab-find form input.text{position:absolute;left:8px;top:8px;width:140px;height:16px;line-height:16px;}
div.tab-control div.tab-find ul{margin-right:10px;margin-top:4px;}
div.tab-control div.tab-find ul li{padding-left:25px;background-image:url("../icons/tab-control_tab-find_li.png");background-repeat:no-repeat;}
div.tab-control div.tab-find ul a{width:24px;height:24px;background-image:url("../icons/tab-control_tab-find_li.png");background-position:0 -60px;}
div.tab-control div.main{position:relative;}
div.tab-control div.main iframe{width:100%;height:100%;box-shadow: 0 0 1px #EDC9C9;background-color:#fff;}
div.tab-control div.tab li{color:#326c2f;}
div.tab-control div.tab li.visited{color:#fff;border:0px;}
div.tab-control div.tab.simple form{display:none;}
div.tab-control div.tab.simple ul{margin:0;}
div.tab-control div.tab input.scroll{cursor:pointer;}
div.tab-control div.tab input.scroll.prev{background-position:0 -40px;}
div.tab-control div.tab input.scroll.next{background-position:-40px -40px;}
div.tab-control div.tab li{z-index:1;}
div.tab-control div.tab li.visited{background-position:0 4px;height: 35px;margin-top: 1px;line-height: 40px;}
div.tab-control div.tab li.visited a{background-position:-140px -5px;top:11px;}
div.tab-control div.tab-find.hidden{display:none;}
div.tab-control div.tab-find form.hover{background-position:0 -30px;}
div.tab-control div.tab-find form.visited{background-position:0 -60px;}
div.tab-control div.tab-find li a{display:none;}
div.tab-control div.tab-find li:hover a{display:inline-block;}
div.tab-control div.tab-find li.true{background-position:0 -30px;}
div.tab-control div.main iframe.visited{display:none;}
div.tab-control div.tab{_width:100%;}
div.tab-control div.tab-find{_right:29px;_top:0;border-right:2px solid #000\9;border-bottom:2px solid #000\9;}
div.tab-control div.tab-find li a{_display:inline-block;}

/** tree样式 **/
div.tree_div{
    border: 0;
    background-color: #fff;
    margin-left: 0px;
    overflow: auto;
    cursor:pointer;
}

div.tree_div ul {
    list-style: none;
    padding-left: 0px;
    margin: 0 2px 1px 0;
}

div.tree_div .tree_li_div {
    height:28px;
    overflow: hidden;
}

div#materialTypeTree .root li ul {
    padding-left:22px;
}

/** 叶子节点图片 **/
div.tree_div ul span.tree_point {
    width:17px;
    height:12px;
    background-position: 0px -282px;
    margin: 8px 0px 8px 5px;
    
}

/** 非叶子节点图片_展开状态 **/
div.tree_div ul span.expand {
    width:17px;
    height:12px;
    background-position: 0px -365px;
    margin: 8px 0px 8px 5px;  
}

/** 非叶子节点图片_收回状态 **/
div.tree_div ul span.expand_no {
    width:17px;
    height:12px;
    background-position: 0px -355px;
    margin: 8px 0px 8px 5px;  
}

div.tree_div ul span.tree_icon {
    background-image:url("./../icons/icon_group.png");
    background-repeat:no-repeat;
    display:block;
    float:left;
}
div.tree_div ul span.icon_open {
    width:17px;
    height:12px;
    background-position: 0px -342px;
    margin: 8px 0px 8px 5px;  
}
div.tree_div ul span.icon_close {
    width:17px;
    height:12px;
    background-position: 0px -330px;
    margin: 8px 0px 8px 5px;  
}

div.tree_div ul span.node_name{
  float: left;
  height: 28px;
  line-height: 28px;
}