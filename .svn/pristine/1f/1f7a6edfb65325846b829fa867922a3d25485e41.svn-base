<#setting classic_compatible=true>
<@insert template="login/right">
    <div class="add_page" style="height:65%;">
        <div class="storage_text_div common_text_div"></div>
        <div class="storage_text_div common_text_div"></div>
        <div class="storage_text_div common_text_div">
            <div class="common_text_title"><span data-required="true">单位名称</span></div>
            <div class="common_text_val"><input id="unitName" class="input_text_common my_radius" value="${materialUnit.unitName}"><input id="id" type="hidden" value="${materialUnit.id}"></div>
        </div>
        <div class="storage_text_div common_text_div">
            <div class="common_text_title"><span data-required="true">单位编号</span></div>
            <div class="common_text_val"><input id="unitNo" class="input_text_common my_radius" value="${materialUnit.unitNo}"></div>
        </div>
        <div class="storage_text_div common_text_div">
            <div class="common_text_title">单位描述</span></div>
            <div class="common_text_val"><input id="description" class="input_text_common my_radius" value="${materialUnit.description}"></div>
        </div>
    </div>
    <div class="user_btns_l common_text_div">
        <button id="material_unit_smt_edit" class="btn_common my_radius btn_submit">保存</button>
        <button id="goback_btn" class="btn_common my_radius">返回</button>
    </div>
</@insert>