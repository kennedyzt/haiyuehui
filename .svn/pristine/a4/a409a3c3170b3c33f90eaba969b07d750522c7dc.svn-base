<#setting classic_compatible=true>
<@insert template="login/right">
    <div class="search_section">
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">关键字</div>
            <div class="common_text_val"><input id="" class="input_text_common my_radius" data-click-tip="输入单据编号、商品、所有人关键字" placeholder="单据编号、商品、所有人"></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">单据日期(从)</div>
            <div class="common_text_val"><input id="" class="input_text_common my_radius" data-type="date"></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">单据日期(至)</div>
            <div class="common_text_val"><input id="" class="input_text_common my_radius" data-type="date"></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">单据状态</div>
            <div class="common_text_val">
                <select id="" class="my_radius">
                    <option value=""></option>
                    <option value="1">结算</option>
                    <option value="2">未结算</option>
                </select>
            </div>
        </div>
        <div class="search_btn common_text_div">
            <button class="btn_common my_radius btn_search">搜索</button>
        </div>
        <div style="clear:both"></div>
    </div>
    <table>
        <thead>
            <tr>
                <td width="10%"><a class="td_head_a" onclick="deleteDraftApplyPO()" data-hover-tip="删除"><img src="<@static value='/icons/delete.png' />"></a></td>
                <td width="9%">单据编号</td>
                <td width="9%">单据日期</td>
                <td width="9%">有效期至</td>
                <td width="9%">必须日期</td>
                <td width="9%">申请者</td>
                <td width="9%">商品货号</td>
                <td width="9%">商品名称</td>
                <td width="9%">商品国际编码</td>
                <td width="9%">计量单位</td>
                <td width="9%">单据摘要</td>
            </tr>
        </thead>
    </table>
    <div class="data_grid">
        <table>
            <tbody>
                <tr>
                    <td width="10%">
                        <input id="" type="radio" name="check_box">
                        <a class="td_inner_a" href="<@url value='#' />" data-hover-tip="修改"><img src="<@static value='/icons/edit.png' />"></a>
                    </td>
                    <td width="9%"><a href="<@url value='#' />" data-hover-tip="查看详情">4542121212112</a></td>
                    <td width="9%">单据日期</td>
                    <td width="9%">有效期至</td>
                    <td width="9%">必须日期</td>
                    <td width="9%">申请者</td>
                    <td width="9%">商品货号</td>
                    <td width="9%">商品名称</td>
                    <td width="9%">商品国际编码</td>
                    <td width="9%">计量单位</td>
                    <td width="9%">单据摘要</td>
                </tr>
            </tbody>
        </table>
    </div>
    <div class="order_btns_l common_text_div">
        <button id="goback_btn" class="btn_common my_radius">返回</button>
    </div>
</@insert>