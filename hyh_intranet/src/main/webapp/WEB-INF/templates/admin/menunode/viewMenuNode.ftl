<#setting classic_compatible=true>
<@insert template="login/right">
    <div class="add_page" style="height:50%;">
        <input type="hidden" id="id"  value="${menunode.id}"/>
        <div class="user_text_div common_text_div">
            <div class="common_text_title">菜单名称</div>
            <div class="common_text_val"><span class="readonly_text_common grid_page_text" id="">${menunode.name}</span></div>
        </div>
        <div class="user_text_div common_text_div">
            <div class="common_text_title">菜单链接</div>
            <div class="common_text_val"><span class="readonly_text_common grid_page_text" id="">${menunode.nodeUrl}</span></div>
        </div>
        <div class="user_text_div common_text_div">
            <div class="common_text_title">菜单排序</div>
            <div class="common_text_val"><span class="readonly_text_common grid_page_text" id="">${menunode.sequence}</span></div>
        </div>
        <div class="user_text_div common_text_div">
            <div class="common_text_title">菜单等级</div>
            <div class="common_text_val"><span class="readonly_text_common grid_page_text" id="">${menunode.level}</span></div>
        </div>
        <div class="user_text_div common_text_div">
            <div class="common_text_title">上级菜单</div>
            <div class="common_text_val"><span class="readonly_text_common grid_page_text" id="">${menunode.parentName}</span></div>
        </div>
        <div class="user_text_div common_text_div">
            <div class="common_text_title">菜单描述</div>
            <div class="common_text_val"><span class="readonly_text_common grid_page_text" id="">${menunode.description}</span></div>
        </div>
    </div>
</@insert>