<#setting classic_compatible=true>
<@insert template="login/right">
    <div class="add_page">
        <div class="partner_text_div common_text_div" style="width: 100%;">
            <div class="section_title_0 section_part_1" style="text-align: center;">基础</div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">编码</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.partnerCode}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">类型</div>
            <div class="common_text_val"><span class="text_val"><#if businessPartner.partnerType==1>客户</#if><#if businessPartner.partnerType==2>供应商</#if></span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">名称</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.partnerName}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">外文名称</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.foreignName}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">组</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.partnerGroupName}</div>
        </div>
        <div class="partner_text_div common_text_div" style="width: 100%;">
            <div class="section_title_0 section_part_1" style="text-align: center;">常规</div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">电话</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.phone}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">移动电话</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.mobilephone}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">传真</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.fax}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">电子邮件</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.email}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">网址</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.website}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">是否可用</div>
            <div class="common_text_val"><span class="text_val"><#if businessPartner.isEnable==true>可用<#else>不可用</#if></span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">企业地址(中文)</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.address}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">企业地址(英文)</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.addressEn}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">国家/地区</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.countryRegionName}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">企业类型</div>
            <div class="common_text_val"><span class="text_val"><#if businessPartner.businessType == 1>出口商<#elseif businessPartner.businessType == 2>代理商<#elseif businessPartner.businessType == 3>出口商/代理商</#if></span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">邮政编码</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.postalCode}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">备注</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.description}</span></div>
        </div>
        <div class="partner_text_div common_text_div" style="width: 100%;">
            <div class="section_title_0 section_part_1" style="text-align: center;">联系人</div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">姓名</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.contactsFirstName}${businessPartner.contactsLastName}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">性别</div>
            <div class="common_text_val">
                <div class="common_text_val"><span class="text_val">男</span></div>
            </div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">电话</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.contactsPhone}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">移动电话</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.contactsMobilephone}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">传真</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.contactsFax}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">邮箱</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.contactsEmail}</span></div>
        </div>
        <div class="partner_text_div common_text_div">
            <div class="common_text_title">备注</div>
            <div class="common_text_val"><span class="text_val">${businessPartner.contactsDescription}</span></div>
        </div>
    </div>
    <div class="order_btns_l common_text_div">
        <button id="goback_btn" class="btn_common my_radius">返回</button>
    </div>
</@insert>